{
	"name": "obsidian-hatena-expressive",
	"version": "1.0.0",
	"description": "Post Obsidian notes to Hatena Blog with Expressive Code syntax highlighting",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint ."
	},
	"keywords": [
		"obsidian",
		"hatena",
		"blog",
		"expressive-code",
		"syntax-highlighting"
	],
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@types/he": "^1.2.0",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1",
		"@eslint/js": "9.30.1",
		"jiti": "2.6.1"
	},
	"dependencies": {
		"obsidian": "latest",
		"unified": "^11.0.0",
		"remark-parse": "^11.0.0",
		"remark-frontmatter": "^5.0.0",
		"remark-rehype": "^11.0.0",
		"rehype-stringify": "^10.0.0",
		"rehype-raw": "^7.0.0",
		"rehype-expressive-code": "^0.41.0",
		"unist-util-visit": "^5.0.0",
		"yaml": "^2.3.0",
		"he": "^1.2.0",
		"mime": "^4.0.0"
	}
}
